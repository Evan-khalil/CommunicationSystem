<!DOCTYPE html>
<html lang="sv">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Kommunikationssystem</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/lib/fontawesome/css/all.css" />
    <link href="~/lib/fullcalendar/main.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/lib/ekko-lightbox/ekko-lightbox.css">
    <link rel="stylesheet" href="~/css/site.css" />
</head>
<body>
<header>
    <partial name="_Navbar" />
</header>
<div class="container">
    <main role="main" class="pb-3">
        @RenderBody()
    </main>
</div>
<footer class="border-top footer text-muted">
    <div class="container text-center">
        &copy; @DateTime.Now.Year - Kommunikationssystem - <a asp-controller="Home" asp-action="Privacy">Integritetspolicy</a> - <a asp-controller="Home" asp-action="Support">Support</a>
    </div>
</footer>
<script src="~/lib/jquery/dist/jquery.min.js"></script>
<script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
<script src="~/lib/fullcalendar/main.js"></script>
<script src="~/lib/moment/moment.min.js"></script>
<script src="~/lib/alertify/js/alertify.js"></script>
<script src="~/js/site.js" asp-append-version="true"></script>
@await RenderSectionAsync("Scripts", required: false)
</body>
</html>